ProgressBar.BackgroundColor3 = Color3.fromRGB(255, 255, 255)
ProgressBar.Size = UDim2.new(0, 0, 1, 0)

-- Configuración del UICorner para esquinas redondeadas del ProgressBar
local ProgressBarCorner = Instance.new("UICorner")
ProgressBarCorner.CornerRadius = UDim.new(0, 10)
ProgressBarCorner.Parent = ProgressBar

-- Animar la barra de progreso
local function animateProgressBar()
    for i = 0, 1, 0.01 do
        ProgressBar.Size = UDim2.new(i, 0, 1, 0)
        wait(0.01)  -- Ajuste de tiempo para una animación más fluida
    end
end

-- Mostrar la interfaz, animar la barra de progreso y luego eliminarla
animateProgressBar()
wait(1)
ScreenGui:Destroy()
