local function validateKey(key)
    return key == "spfORvhtui2hMn5Lliwy4LtV1ppu6HbExbN9G24psjAMjk6wWRjqZBDQiE5ld504JCDwKUqePdVTW0CQ25zkd8oOWoha6zC2Hq7StwYeom4uocN2RifT51mjmyXvvTQNCBFj13cV5mGz9lZsJuu4RV" -- Replace this with your key
end

CheckKey.MouseButton1Click:Connect(function()
    local enteredKey = TextBox.Text
    if validateKey(enteredKey) then
        TextBox.PlaceholderText = "CR7 Scripting"
        TextBox.Text = ""
        wait(1)
        ScreenGui:Destroy()

